{
  "name": "wip-apple-api-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json}'",
    "test": "jest",
    "clean": "git clean -fxd -e .vscode -e .env.local",
    "clean:install": "yarn clean && yarn",
    "clean:install:dev": "yarn clean:install && yarn dev",
    "clean:hard": "yarn clean:lock && yarn clean && yarn clean:node_modules",
    "clean:hard:install": "yarn clean:hard && yarn",
    "clean:dist": "rm -rf .next",
    "clean:lock": "rm -rf yarn.lock",
    "clean:node_modules": "rm -rf node_modules"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.20.0",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-infinite-scroll-component": "^5.1.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.3",
    "@types/react": "^16.9.49",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.2"
  }
}
